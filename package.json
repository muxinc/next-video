{
	"name": "next-video",
	"version": "1.0.0",
	"description": "The easiest way to get started with video in your Next.js application. This library will automatically upload your videos to Mux and serve them from their CDN.",
	"main": "dist/NextVideo.js",
	"scripts": {
		"clean": "rm -rf dist",
		"build": "npm run clean && swc src -d dist -s && tsc"
	},
	"bin": {
		"main": "bin/main.js"
	},
	"type": "module",
	"author": "Mux Lab <lab@mux.com>",
	"license": "MIT",
	"peerDependencies": {
		"@types/react": "^17.0.0 || ^18",
		"react": "^17.0.2 || ^18",
		"react-dom": "^17.0.2 || ^18"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		},
		"@types/react-dom": {
			"optional": true
		}
	},
	"devDependencies": {
		"@parcel/packager-ts": "^2.9.3",
		"@parcel/transformer-typescript-types": "^2.9.3",
		"@types/node": "^20.4.5",
		"@types/react": "^18.2.16",
		"parcel": "^2.9.3",
		"react": "^18.2.0",
		"swc": "^1.0.11",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"@mux/mux-node": "github:stainless-sdks/mux-node#8.0.0-beta.1",
		"@mux/mux-player-react": "^1.11.4",
		"chokidar": "^3.5.3",
		"undici": "^5.22.1"
	}
}
