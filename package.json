{
  "name": "@mux/next-video",
  "version": "0.4.0",
  "description": "A React component for adding video to your Next.js application. It extends both the video element and your Next app with features for automatic video optimization.",
  "type": "module",
  "main": "./dist/cjs/next-video.js",
  "module": "./dist/next-video.js",
  "exports": {
    ".": {
      "import": "./dist/next-video.js",
      "require": "./dist/cjs/next-video.js",
      "default": "./dist/next-video.js"
    },
    "./process": {
      "import": "./dist/main.js",
      "require": "./dist/cjs/main.js",
      "default": "./dist/main.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "watch": "npm run build:esm -- -w & npm run build:cjs -- -w",
    "build": "npm run clean && (npm run build:esm && npm run build:cjs)",
    "build:esm": "tsc --module es2020",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "postbuild:cjs": "echo '{\"type\": \"commonjs\"}' > ./dist/cjs/package.json",
    "prepare": "npm run build",
    "cli": "node --loader tsx --no-warnings ./src/cli",
    "test": "glob -c \"node --loader tsx --no-warnings --test\" \"./tests/**/*.test.ts\""
  },
  "bin": "./dist/cli.js",
  "files": [
    "dist",
    "video-types"
  ],
  "author": "Mux Lab <lab@mux.com>",
  "license": "MIT",
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18",
    "react": "^17.0.2 || ^18",
    "react-dom": "^17.0.2 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.2.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.16",
    "@types/yargs": "^17.0.24",
    "glob": "^10.3.3",
    "react": "^18.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.0.2",
    "@mux/mux-node": "github:stainless-sdks/mux-node#8.0.0-beta.1",
    "@mux/mux-player-react": "1.12.1-canary.0-e90e096",
    "@next/env": "^13.4.19",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "magicast": "^0.2.10",
    "sharp": "^0.32.5",
    "symlink-dir": "^5.1.1",
    "thumbhash": "^0.1.1",
    "undici": "^5.22.1",
    "yargs": "^17.7.2"
  }
}
